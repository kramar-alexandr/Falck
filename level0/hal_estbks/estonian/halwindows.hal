procedure BKGTransVClass_Tool(Integer cnt,var real v,var real vm,var real h)
begin
  if (cnt==11) then begin  
    if (v>vm) then begin vm = v; end;
    h = 200;
    v = 58;
  end;
  if (cnt==22) then begin  
    if (v>vm) then begin vm = v; end;
    h = 380;
    v = 58;
  end;
  return;
end;

