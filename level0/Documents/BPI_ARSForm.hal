//                                  vaCurncyCode,vacnt);
		
		
		//-------------------------SUPPLIER
		for (i=0;i<vacnt;i=i+1) begin
			if(vaInvDate[i]==adates[ii])then begin
				AddCurncyTotals(acur,acurv1,vcurcnt,acur[i],vaRVal[i]);
				PrintValueInclZero("F_FAKTBELOPP",vaRVal[i],M4Val,LangNrr,SFb,false);
				
				tstr = vaInvoiceNr[i];
