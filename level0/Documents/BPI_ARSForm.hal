procedure SortDateArray(var array date inputa)
begin
	integer i,j;
	date d1,d2;
	
	if(inputa.length>1)then begin
		for(j=0;j<inputa.length;j=j+1)begin
			for(i=1;i<inputa.length;i=i+1)begin
				if(inputa[i]<inputa[i-1])begin
					d1 = inputa[i];
					d2 = inputa[i-1];
					inputa[i] = d2;
					inputa[i-1] = d1;
				end;
			end;
		end;
	end;

return;
end;

